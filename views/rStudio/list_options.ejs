<%- partial ('../shared/_navbar.ejs') %>
<% var topics = data; %>
<section class="package">
  <div class="container">
    <header>
      <h4>Multiple matching functions</h4>
    </header>
    <section class="table-list">
      <input id="filter" type="text" placeholder="Filter list">
      <table>
        <thead>
          <tr>
            <td>Name</td>
            <td>Package</td>
            <td>Description</td>
          </tr>
        </thead>
        <tbody id="filterableItems">
          <% for(var i = 0; i < topics.length; i++) { %>
            <% var topic = topics[i]; %>
            <tr>
              <td><a href="<%= '/topics/' + topic.id %>"><%= topic.function_name %> (alias:<%=topic.function_alias%>)</a></td>
              <td><%- topic.package_name %></td>
              <td><%- topic.function_description%></td>
            </tr>
          <% } %>
          <tr class="no-results"><td colspan="3">No Results!</td></tr>
        </tbody>
      </table>
    </section>  
  </div>
</section>
<%- partial ('../shared/_footer.ejs') %>